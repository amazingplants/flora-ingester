  "scripts": {
    "wfo": "NODE_ENV=production node dist/wfo --max-old-space-size=8192",
    "wfo-dev": "tsc && NODE_ENV=development node dist/wfo",
    "powo": "NODE_ENV=production node dist/powo --max-old-space-size=8192",
    "powo-dev": "tsc && NODE_ENV=development node dist/powo",
    "powo-fetch": "NODE_ENV=production node dist/powo/fetch --max-old-space-size=8192",
    "powo-fetch-dev": "tsc && NODE_ENV=development node dist/powo/fetch",
    "wfo-fetch": "NODE_ENV=production node dist/wfo/fetch --max-old-space-size=8192",
    "wfo-fetch-dev": "tsc && NODE_ENV=development node dist/wfo/fetch",
    "build": "tsc",
    "test": "tsc && NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx jest --runInBand",
    "test-wfo": "tsc && NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx jest wfo/ingester --runInBand",
    "test-powo": "tsc && NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx jest powo/ingester --runInBand",
    "test-watch": "tsc -w && NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx jest --watch --runInBand"
